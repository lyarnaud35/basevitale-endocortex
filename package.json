    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.7.0",
    "neo4j-driver": "^6.0.1",
    "nx": "19.0.0",
    "orval": "^7.21.0",
    "postcss": "^8.4.32",
    "prettier": "^2.6.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.0",
