    "@nestjs/testing": "^10.0.0",
    "@nx/eslint": "19.0.0",
    "@nx/eslint-plugin": "19.0.0",
    "@nx/jest": "19.0.0",
    "@nx/js": "^19.8.14",
    "@nx/nest": "19.0.0",
    "@nx/next": "19.0.0",
    "@nx/node": "19.0.0",
    "@nx/react": "19.0.0",
