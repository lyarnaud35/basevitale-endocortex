{
  "name": "basevitale",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "clean-api": "bash ./scripts/clean-api.sh",
    "build:api": "./scripts/build-api.sh",
    "build:api:clean": "./scripts/build-api.sh --clean",
    "test": "nx test",
    "lint": "nx lint",
    "dev": "NODE_OPTIONS='--no-deprecation' nx serve api",
    "dev:api": "NODE_OPTIONS='--no-deprecation' nx serve api",
    "dev:api-only": "./scripts/start-api-only.sh",
    "dev:web": "NODE_OPTIONS='--no-deprecation' nx serve web",
    "prisma:generate": "cd apps/api && npx prisma generate",
    "prisma:push": "cd apps/api && npx prisma db push",
    "prisma:migrate": "cd apps/api && npx prisma migrate dev",
    "prisma:studio": "cd apps/api && npx prisma studio",
    "ingest:bdpm": "cd apps/api && npx ts-node -P tsconfig.app.json src/commands/ingest-bdpm.ts --download",
    "ingest:bdpm:local": "cd apps/api && npx ts-node -P tsconfig.app.json src/commands/ingest-bdpm.ts --no-download",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "./scripts/setup-and-test.sh",
    "dev:setup": "./scripts/dev-setup.sh",
    "validate:env": "./scripts/validate-env.sh",
    "validate:boucle": "./scripts/validate-boucle-intelligence.sh",
    "test:sprint2": "./scripts/test-sprint2.sh",
    "test:integration": "./scripts/test-integration-complete.sh",
    "test:ai-cortex": "./scripts/test-ai-cortex.sh",
    "test:tracer-bullet": "./scripts/test-scribe-mock.sh",
    "test:phase-c": "./scripts/test-phase-c-local.sh",
    "test:scribe": "nx test api --testPathPattern=scribe",
    "test:e2e": "./scripts/test-e2e.sh",
    "check:config": "./scripts/check-config.sh",
    "check:node20": "bash ./scripts/check-node-20.sh",
    "phase2:clean-install": "bash ./scripts/phase2-clean-install.sh",
    "start:complete": "./scripts/start-complete.sh",
    "clean-start": "kill -9 $(lsof -t -i:3000) 2>/dev/null || true && docker stop $(docker ps -q) 2>/dev/null || true && npx nx serve api",
    "sdk:gen": "orval",
    "openapi:fetch": "bash ./scripts/openapi-fetch.sh",
    "sdk:gen:file": "node ./scripts/generate-sdk-standalone.js",
    "sdk:gen:docker": "bash ./scripts/sdk-gen-docker.sh"
  },
  "private": true,
  "dependencies": {
